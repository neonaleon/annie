<li class="dashboard-item" data-metric-id="{{_id}}" data-col="1" data-row="1" data-sizex="1" data-sizey="2">
  <h1>{{name}}</h1>
  <table class="table table-striped table-hover">
    <thead>
      <tr>
      {{#with value}}
        {{#each labels}}
          <th>{{this}}</th>
        {{/each}}
      {{/with}}
      </tr>
    </thead>
    <tbody>
      {{#with value}}
        {{#each data}}
          <tr>
            {{#each this}}
            <td>{{this}}</td>
            {{/each}}
          </tr>
        {{/each}}
      {{/with}}
    </tbody>
  </table>
</li>