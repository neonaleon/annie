<li class="dashboard-item" data-metric-id="{{_id}}" data-col="1" data-row="1" data-sizex="2" data-sizey="2">
  <h1>{{name}}</h1>
  <div class="metric-flot" data-metric-type="bar">
    <script class="metric-data" type="text/x-metric-data">
    {{{value}}}
    </script>
    <canvas></canvas>
  </div>
</li>